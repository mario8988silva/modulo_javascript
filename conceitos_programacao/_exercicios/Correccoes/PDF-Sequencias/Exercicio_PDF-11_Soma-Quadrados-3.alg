inicio
    real a
    real b
    real c
    
    escrever "Soma dos quadrados de 3 valores" , "\n"
    
    escrever "Indicar o primeiro valor: " , "\n"
    ler a
    
    escrever "Indicar o segundo valor: " , "\n"
    ler b
    
    escrever "Indicar o terceiro valor: " , "\n"
    ler c
    
    escrever "Resultado: " , a ^ 2 + b ^ 2 + c ^ 2
fim

