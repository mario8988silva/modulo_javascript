inicio
    inteiro a
    inteiro b
    inteiro troca
    
    escrever "Trocar valores A -> B" , "\n"
    
    escrever "Indicar o primeiro valor: " , "\n"
    ler a
    
    escrever "Indicar o segundo valor: " , "\n"
    ler b
    
    troca <- a
    a <- b
    b <- troca
    
    escrever "Valor de A: " , a , "\n"
    escrever "Valor de B: " , b
fim
