
inicio
    texto nome
    texto genero
    
    escrever "Qual o seu género? (m/f)"
    ler genero
    
    escrever "Qual o seu nome?"
    ler nome
    
    escolhe genero
        caso "m":
            escrever "Bom dia Sr." nome
        caso "f":
            escrever "Bom dia Sra."
    fimescolhe
fim
