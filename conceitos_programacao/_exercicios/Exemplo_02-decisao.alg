inicio
caracter genero
    escrever "Qual o seu género? (m / f)"
    ler genero
    escrever "Olá"
    
    escolhe genero
        caso "m":
            escrever " Homem"
        caso "f":
            escrever " Mulher"
        defeito:
            escrever "genero indefinido"
        fimescolhe
        
    escrever ", bem-vindo"
fim
        
        