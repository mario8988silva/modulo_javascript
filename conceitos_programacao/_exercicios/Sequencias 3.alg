
inicio
    real f
    real c
    real x
    real y
    
    escrever "Insira a temperatura em Fahrenheit: "
        ler f
    
    x <- f - 32
    escrever x, "                        "
    
    y <- 5 / 9
    escrever y, "                        "       
    
    c <- x * y
    escrever c, "                        "
fim
